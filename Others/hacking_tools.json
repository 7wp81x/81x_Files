"msfconsole":{
"name":"Metasploit","packages": [
    "libiconv", "zlib", "autoconf", "bison", "clang", "coreutils",
    "curl", "findutils", "git", "apr", "apr-util", "libffi", "libgmp",
    "libpcap", "postgresql", "readline", "libsqlite", "openssl", "libtool",
    "libxml2", "libxslt", "ncurses", "pkg-config", "wget", "make", "libgrpc",
    "termux-tools", "ncurses-utils", "ncurses", "unzip", "zip", "tar",
    "termux-elf-cleaner", "binutils","cmake", "libandroid-execinfo",
    "libandroid-support","libyaml","perl", "libllvm"
],
"cmd": "",
"des": "Penetration testing framework for identifying and exploiting vulnerabilities across various platforms"},

"rsf": {
    "name":"RouterSploit",
    "packages":[
        "python-cryptography", "libsodium","automake",
        "binutils","autoconf","rust"
    ],
    "cmd": "git clone https://github.com/threat9/routersploit && cd routersploit && pip3 install -r requirements.txt && pkg uninstall rust -y",
    "des": "Exploitation framework focused on testing vulnerabilities in routers and embedded devices."
},

"phpsploit": {
    "name":"PHPSploit",
    "packages": [],
    "cmd": "git clone https://github.com/nil0x42/phpsploit && cd phpsploit && pip3 install -r requirements.txt",
    "des": "Remote control framework written in PHP for penetration testing and web-based exploitation."
},

"dirsearch": {
    "name":"Dirsearch",
    "packages": ["python-cryptography"],
    "cmd": "pip3 install disearch",
    "des": "Designed to brute force directories and files on web servers."
},

"aircrack-ng": {
    "name":"Aircrack-ng",
    "packages": [
        "libc++","libnl","libpcap","libsqlite","openssl","pcre","zlib"
    ],
    "cmd": "wget 'https://github.com/7wp81x/81x_Files/blob/main/Others/Aircrack/aircrack-ng_3_1.7_$(uname -m).deb?raw=true' -O aircrack.deb && dpkg -i aircrack.deb",
    "des": "Use for cracking wireless network (cap/pcaps)"
},

"binwalk": {
    "name":"Binwalk",
    "packages": [],
    "cmd": "pip install git+https://github.com/ReFirmLabs/binwalk.git",
    "des": "Tool used for analyzing, extracting, and reverse engineering firmware images."
},

"maskphish": {
    "name":"MaskPhish",
    "packages": [],
    "cmd": "wget https://raw.githubusercontent.com/jaykali/maskphish/master/maskphish.sh -O $PREFIX/bin/maskphish && chmod 777 $PREFIX/bin/maskphish",
    "des": "Masking phishing urls."
},

"wfuzz": {
    "name":"wFuzz",
    "packages":[],
    "cmd": "pip3 install wfuzz",
    "des": "Web application security tool for brute-forcing and fuzz testing to find vulnerabilities, hidden directories, files, and parameters."
},

"kr": {
    "name":"Kite Runner",
    "packages": ["golang"],
    "cmd": "git clone https://github.com/assetnote/kiterunner && cd kiterunner && make build-linux && mv dist/kr-linux $PREFIX/bin/kr && chmod 777 $PREFIX/bin/kr",
    "des": "Tool for reconnaissance in web applications and APIs, focusing on endpoint discovery using customizable wordlists and request templates."
},

"apk-mitm": {
    "name":"APK MITM",
    "packages":["nodejs"],
    "cmd": "npm install -g apk-mitm",
    "des": "Tool mainly made for patching apk files to bypass SSL pinning."
},

"seeker": {
"name":"Seeker",
"packages":["php"],
"cmd": "git clone https://github.com/thewhiteh4t/seeker && cd seeker && pip3 install requests packaging psutil",
"des": "Location tracking through social engineering."
},

"hashbuster": {
    "name":"HashBuster",
    "packages":[],
    "cmd": "wget https://raw.githubusercontent.com/s0md3v/Hash-Buster/master/hash.py -O $PREFIX/bin/buster && chmod 777 $PREFIX/bin/buster",
    "des": "Check hash if it's cracked via various sources."
},

"zphisher": {
    "name":"Zphisher",
    "packages": ["php", "openssl", "openssh"],
    "cmd": "wget https://github.com/htr-tech/zphisher/releases/download/2.3.5/zphisher_2.3.5_all_termux.deb && dpkg -i zphisher_2.3.5_all_termux.deb && rm zphisher_2.3.5_all_termux.deb",
    "des": "Automated phishing tool that simplifies the process of creating phishing attacks to capture sensitive information like login credentials."},

"ngrok": {
    "name":"Ngrok",
    "packages": "",
    "cmd": "wget https://bin.equinox.io/c/bNyj1mQVY4c/ngrok-v3-stable-linux-arm64.tgz -O ngrok.tgz && tar xvzf ngrok.tgz -C $PREFIX/bin/ && rm ngrok.tgz",
    "des": "Port forwarding"
}
